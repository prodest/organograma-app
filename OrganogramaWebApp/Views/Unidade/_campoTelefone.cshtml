@model WebApp.Models.TelaUnidadeModel

<div class="form-group col-md-4">
    @Html.LabelFor(model => model.contatos[0].idTipoContato, htmlAttributes: new { @class = "control-label", @For = "contatos["+ViewBag.i+"].idTipoContato" })
    @Html.DropDownListFor(model => model.contatos[0].idTipoContato, new SelectList(Model.listaTiposContato, "id", "descricao", "quantidadeDigitos"), "Selecione um Tipo", new { @class = "form-control", @Name = "contatos[" + ViewBag.i + "].idTipoContato", @Id = "contatos[" + ViewBag.i + "].idTipoContato" })
    @Html.ValidationMessageFor(model => model.contatos[0].idTipoContato, "", new { @class = "text-danger" })
</div>
<div class="form-group col-md-8">
    @Html.LabelFor(model => model.contatos[0].telefone, htmlAttributes: new { @class = "control-label", @For = "contatos[" + ViewBag.i + "].telefone" })
    @Html.EditorFor(model => model.contatos[0].telefone, new { htmlAttributes = new { @class = "form-control", @Name = "contatos[" + ViewBag.i + "].telefone", @Id = "contatos[" + ViewBag.i + "].telefone" } })
    @Html.ValidationMessageFor(model => model.contatos[0].telefone, "", new { @class = "text-danger", @Name = "contatos[" + ViewBag.i + "].telefone"  })
</div>