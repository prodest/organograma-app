@model ProcessoEletronicoWebApp.Apresentacao.Models.TelaUnidadeModel

<div class="form-group">
    @Html.LabelFor(model => model.sites[0].url, htmlAttributes: new { @class = "control-label", @For = "sites[" + ViewBag.i + "].url" })
    @Html.EditorFor(model => model.sites[0].url, new { htmlAttributes = new { @class = "form-control", @Name = "sites["+ ViewBag.i +"].url" } })
    @Html.ValidationMessageFor(model => model.sites[0].url, "", new { @class = "text-danger" })
</div>